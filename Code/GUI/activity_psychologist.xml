<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Psychologist">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/gradient"
        tools:context=".HeadmasterHome"
        tools:layout_editor_absoluteX="-51dp"
        tools:layout_editor_absoluteY="0dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/gradient"
            tools:context=".HeadmasterHome"
            tools:layout_editor_absoluteX="-65dp"
            tools:layout_editor_absoluteY="0dp">

            <ImageView
                android:id="@+id/logout"
                android:layout_width="211dp"
                android:layout_height="158dp"
                android:layout_below="@+id/hname"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="98dp"
                android:layout_marginTop="250dp"
                android:layout_marginEnd="102dp"
                android:layout_marginBottom="68dp"
                app:layout_constraintVertical_bias="0.811"
                app:srcCompat="@drawable/balashero" />

            <TextView
                android:id="@+id/headTitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginStart="50dp"
                android:layout_marginTop="70dp"
                android:layout_marginEnd="50dp"
                android:layout_marginBottom="50dp"
                android:gravity="center"
                android:text="Hello Psychologist:"
                android:textColor="@color/white"
                android:textSize="35dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/hname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/headTitle"
                android:layout_marginStart="50dp"
                android:layout_marginTop="25dp"
                android:layout_marginEnd="50dp"
                android:layout_marginBottom="50dp"
                android:background="#30ffffff"
                android:gravity="center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="16dp" />

            <ImageButton
                android:id="@+id/hprofile"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_below="@+id/hname"
                android:layout_alignParentStart="true"
                android:layout_marginStart="72dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/btn_bg"
                android:scaleType="fitCenter"
                android:src="@drawable/huser" />

            <TextView
                android:id="@+id/profile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/hprofile"
                android:layout_alignParentStart="true"
                android:layout_marginStart="82dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="1dp"
                android:text="Profile"
                android:textColor="@color/white"
                android:textSize="10dp" />

            <ImageButton
                android:id="@+id/hcontact"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_below="@+id/hname"
                android:layout_marginStart="53dp"
                android:layout_marginLeft="125dp"
                android:layout_marginTop="8dp"
                android:layout_toEndOf="@+id/hprofile"
                android:background="@drawable/btn_bg"
                android:scaleType="fitCenter"
                android:src="@drawable/phone" />

            <TextView
                android:id="@+id/events"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/hcontact"
                android:layout_marginStart="58dp"
                android:layout_marginLeft="135dp"
                android:layout_marginTop="-1dp"
                android:layout_toEndOf="@+id/profile"
                android:text="Contact Student"
                android:textColor="@color/white"
                android:textSize="10dp" />

            <ImageButton
                android:id="@+id/hprogramm"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_below="@+id/hname"
                android:layout_marginStart="52dp"
                android:layout_marginLeft="225dp"
                android:layout_marginTop="8dp"
                android:layout_toEndOf="@+id/hcontact"
                android:background="@drawable/btn_bg"
                android:scaleType="fitCenter"
                android:src="@drawable/evaluat" />


            <TextView
                android:id="@+id/programm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/hprogramm"
                android:layout_marginStart="27dp"
                android:layout_marginLeft="227dp"
                android:layout_marginTop="-1dp"
                android:layout_toEndOf="@+id/events"
                android:text="Evaluate Progress"
                android:textColor="@color/white"
                android:textSize="10dp" />

            <ImageButton
                android:id="@+id/hevaluate"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_below="@+id/hname"
                android:layout_marginLeft="175dp"
                android:layout_marginTop="105dp"
                android:background="@drawable/btn_bg"
                android:scaleType="fitCenter"
                android:src="@drawable/help" />

            <TextView
                android:id="@+id/help"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/hevaluate"
                android:layout_marginLeft="190dp"
                android:text="Help"
                android:textColor="@color/white"
                android:textSize="10dp" />


        </RelativeLayout>


    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>